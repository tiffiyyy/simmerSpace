{
    "framework": "vite",
    "installCommand": "npm ci",
    "buildCommand": "XR_ENV=avp npm run build",
    "outputDirectory": "dist",
    "env": { "XR_ENV": "avp" },
    "rewrites": [
      { "source": "/assets/(.)", "destination": "/assets/$1" },
      { "source": "/(.)", "destination": "/index.html" }
    ]
  }